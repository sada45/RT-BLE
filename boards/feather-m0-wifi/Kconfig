config BOARD
    default "feather-m0-wifi" if BOARD_FEATHER_M0_WIFI

config BOARD_FEATHER_M0_WIFI
    bool
    default y
    select BOARD_FEATHER_M0_BASE
    select HAVE_ATWINC15X0

source "$(RIOTBOARD)/feather-m0/Kconfig.feather-m0-base"
